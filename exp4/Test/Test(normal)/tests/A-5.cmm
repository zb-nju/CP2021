int ADD(int ai, int aj) {
    return ai + aj;
}

int SUB(int si, int sj) {
    return si - sj;
}

int MUL(int mi, int mj) {
    return mi * mj;
}

int main() {
    int i, j, k, l, res;
    i = read();
    j = read();
    k = read();
    l = read();
    res = ADD(MUL(SUB(i, j), SUB(i, j)), ADD(k, l));
    write(res);
    return 0;
}
